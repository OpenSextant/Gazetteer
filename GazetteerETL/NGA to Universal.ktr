        <class_name>Processor</class_name>

        <class_source><![CDATA[
import java.util.*;


// this step
// 1) creates the ADMIN1ID by concatenating the "main" country code and the admin1 value (ADMIN1ID)
// 2) creates a list of distinct country codes from the cc1 and cc2 fields (COUNTRYCODELIST)
// 3) creates a list from all the distinct names from this record (NAMELIST)

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}


	// create an ouput row with room for three new fields
    r = createOutputRow(r, r.length + 3);


	// Get the country codes
    String cc1 = get(Fields.In, "CC1").getString(r);
	String cc2 = get(Fields.In, "CC2").getString(r);

	// get the admin1 value
	String admin1 = get(Fields.In, "ADM1").getString(r);

	// get the three flavors of place names
	String shortName = get(Fields.In, "SHORT_FORM").getString(r);
	String fullName = get(Fields.In, "FULL_NAME_RO").getString(r);
	String fullNameND = get(Fields.In, "FULL_NAME_ND_RO").getString(r);


	// create an admin1 ID (ADMIN_LEVEL_1) by concatenating the "main" cc with the admin1 value
	// main cc is the single (or first, in case of multiple values) value in cc1 
	// do not use "00" values of admin1 

	String mainCC = cc1.split(",")[0];

	String admin1ID = "";

	if(  admin1 != null && !admin1.equals("00") ){
		admin1ID = mainCC + admin1;
	}

    // Set the new ADMIN1ID output field
    get(Fields.Out, "ADMIN_LEVEL_1").setValue(r, admin1ID);


	// create the list of distinct country codes in cc1 and cc2

	String ccList = mergeCC(cc1,cc2);

	// set the output field
    get(Fields.Out, "COUNTRYCODELIST").setValue(r, ccList);
	


	// concatenate the place names into a comma separated list

	String nameList = fullName;

	// add the FullNameND only if it is non-null and distinct from full name
	if(!fullNameND.equalsIgnoreCase(fullName)){
		nameList = nameList + "|" + fullNameND;
	}

	// add the short name only if non-null and distinct from both full name and full name nd
	if( shortName != null && !shortName.equalsIgnoreCase(fullName) &&  !shortName.equalsIgnoreCase(fullNameND) ){
		nameList = nameList + "|" + shortName;
	}


	// set the NAMELIST output field
    get(Fields.Out, "NAMELIST").setValue(r, nameList);

	// create a PLACE_ID by concatenating the UFI with a constant

	String placeID = "NGA" +get(Fields.In, "UFI").getInteger(r).toString();
	
	// set the placeID
	get(Fields.Out, "PLACE_ID").setValue(r, placeID);


	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}


public String mergeCC(String cc1, String cc2){

	
	if(cc2 == null || cc2.trim().length() <1) {
		cc1 = cc1.replaceAll(",","|");
		return cc1;
	}

	String tmp = cc1 + "," + cc2;
	String[] ccArray = tmp.split(",");

	Set ccSet = new TreeSet();

	for(int i=0; i < ccArray.length; i++){
	   ccSet.add(ccArray[i].trim());
	}

	//turn ccSet back into tab separated list
	Iterator iter = ccSet.iterator();

	String ccList = (String) iter.next();

	while(iter.hasNext()){
		ccList = ccList + "|" + (String) iter.next();
	}

	return ccList;
  }]]></class_source>
