        <class_source><![CDATA[

import org.opensextant.utils.PhoneticUtils;
static double confThresh = 0.9;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();

	if (r == null) {
		setOutputDone();
		return false;
	}

    r = createOutputRow(r, data.outputRowMeta.size());

	// get the name
	String name = get(Fields.In, "PLACE_NAME").getString(r);
	String name_type = get(Fields.In, "NAME_TYPE").getString(r);

	// get the reduction algorithm to be used
	String algo = getParameter("phoneticAlgorithm");

	// phoneticize the name
	String base = "";
	if(name_type.equals("name")){

      base = PhoneticUtils.removeCase(PhoneticUtils.foldToASCII(PhoneticUtils.removeDiacritics(PhoneticUtils.removePunctuation(name))));
	 //  base = phoner.phoneticForm(name, algo);
	}else{
 	//	base = name.toLowerCase().replaceAll("\\.","");
	}

	// prepare base name for tokenization
	base = base.replaceAll("[-\\s]+"," ");

	// get tokencount
	Long tokenCount = new Long(base.split(" ").length);

	get(Fields.Out, "tokenCount").setValue(r, tokenCount);
	get(Fields.Out, "baseName").setValue(r, base);

	putRow(data.outputRowMeta, r);

	return true;
}



