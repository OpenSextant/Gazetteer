        <class_source><![CDATA[public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
		Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }

	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    r = createOutputRow(r, r.length);
	
	String error = get(Fields.In, "Error").getString(r);
	if(error == null){ error = "";}
	String residue = get(Fields.In, "ErrorDetails").getString(r);
	if(residue == null){ residue = "";}


	// get the Feature Class/Code validation fields
    String featClass = get(Fields.In, "Feature_Class").getString(r);
	String featCode = get(Fields.In, "Feature_Code").getString(r);
	String featCombo = featClass + "-" + featCode;

    String mappedCombo = get(Fields.In, "MappedFeatureCombination").getString(r);


	// get the original and mapped country codes
	String ccFIPS = get(Fields.In, "FIPS_COUNTRY_CODE").getString(r);
	String ccISO2 = get(Fields.In, "ISO2_COUNTRY_CODE").getString(r);
	String ccISO3 = get(Fields.In, "ISO3_COUNTRY_CODE").getString(r);

	// check for valid values and combinations

		// look for missing or unmapped country code
		if(ccFIPS == null || ccISO2 == null || ccISO3 == null)  {
		error = error + "," + "NoCountryCode" ;
		}else{
			if(ccFIPS.matches("UNMAPPED") || ccISO2.matches("UNMAPPED") || ccISO3.matches("UNMAPPED"))  {
			error = error + "," + "NoCountryCode" ;
			}
		}


		// check for valid FIPS and ISO ccs

	if( featCode.matches("FAC[0-9]{5}")) {
		// all FAC type codes should be class "S"
		if(!featClass.equals("S")){
			error = error + "," + "BadFeatureClassCodeOrMismatch";
			residue = featCombo;
		}
	}else{

		// check for valid feature code combo 
		if( (mappedCombo == null  || mappedCombo.equals("UNMATCHED") || !mappedCombo.equals(featCombo)) && !featCombo.equals("L-INSM") ) {
		error = error + "," + "BadFeatureClassCodeOrMismatch";
		residue = featCombo;

		}
	}


 	// Set error field
    get(Fields.Out, "Error").setValue(r, error);
	get(Fields.Out, "ErrorDetails").setValue(r, residue);

	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}
]]></class_source>
