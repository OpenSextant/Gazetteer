        <class_source><![CDATA[
import java.util.Set;
import java.util.HashSet;
import org.opensextant.utils.PhoneticUtils;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
		Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }
	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    r = createOutputRow(r, r.length + 1);
	
	String error = get(Fields.In, "Error").getString(r);
	if(error == null){ error = "";}
	String residue = get(Fields.In, "ErrorDetails").getString(r);
	if( residue == null) {residue = "";}

	// get the place name
    String name = get(Fields.In, "PLACE_NAME").getString(r);

	// identify the script(s) used by the name
	Set scripts = PhoneticUtils.scripts(name);

	// set the scripts field
	get(Fields.Out, "SCRIPT").setValue(r, scripts.toString());

	if(!scriptsOK(scripts)) {
		error = error  + "," + "MultipleScripts";
		residue = residue +  + "," +  scripts.toString();
	}

 	// Set error and residue fields
    get(Fields.Out, "Error").setValue(r, error);
 	get(Fields.Out, "ErrorDetails").setValue(r, residue);


	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}


public static boolean scriptsOK(Set scripts){

	// just one = OK
	if(scripts.size() == 1){ return true;}

	// if any combination of CJK, HIRAGANA, KATAKANA = OK
	Set tmp = new HashSet(scripts);
	tmp.remove("CJK");
	tmp.remove("HIRAGANA");
	tmp.remove("KATAKANA");

	if(tmp.size() == 0){ return true;}

	// if any any script with common= OK
	Set tmp2 = new HashSet(scripts);
	tmp2.remove("COMMON");

	if(tmp2.size() == 1){ return true;}

	
 	return false;
}


