    <definitions>
        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }

	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    r = createOutputRow(r, r.length);
	
	String error = get(Fields.In, "Error").getString(r);
	if(error == null){ error = "";}

	// get the coords
    Double lat = get(Fields.In, "LATITUDE").getNumber(r);
	Double lon = get(Fields.In, "LONGITUDE").getNumber(r);

	if( lat == null || lon ==null) {
		error = error + "," + "NullCoords";
	}else{

		if(lat > 90.0 || lat < -90.0 || lon > 180.0 || lon < -180.0 ) {
			error = error + "," + "CoordOutOfRange";
		}

		if( lat == 0.0  &&  lon == 0.0) {
			error = error + "," + "ZeroCoords";
		}
	}


 	// Set error field
    get(Fields.Out, "Error").setValue(r, error);


	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}
]]></class_source>
